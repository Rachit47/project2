<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="PustakaLokam.library.controller.report.MembersWithActiveIssuesReportController"
            prefWidth="600" prefHeight="400">
    <children>
        <Label text="Members with Active Issues" layoutX="180" layoutY="10" style="-fx-font-size: 18px;"/>
        <TableView fx:id="membersTable" layoutX="20" layoutY="50" prefWidth="560" prefHeight="320">
            <columns>
                <TableColumn fx:id="memberIdColumn" text="Member ID" prefWidth="100"/>
                <TableColumn fx:id="nameColumn" text="Name" prefWidth="200"/>
                <TableColumn fx:id="emailColumn" text="Email" prefWidth="260"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>
